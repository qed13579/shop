/**index.wxss**/
page{
  width: 100%;
  height: 100%;
  background: #F9F9F9;
}
.wrap{
  width: 100%;
  height: 100%;
}
.wrap-img{
  height: 188rpx; 
}
.wrap-img image{
  width:100%;
 height: 188rpx; 
}
.wrap-menu{
  width:100%;
  float: left;
}
.menu-img{
  height: 188rpx;
  width:25%;
  float: left;
}
.content-img{
  width: 100%;
  height: 880rpx;
  display: block;
}
.content-img2{
  width: 100%;
  height: 236rpx;
  margin: 0;
  display: block;
}
.product-box{
  width:100%;
  float: left;
}
.product-item{
  float: left;
  height: 376rpx;
  width: 50%;
}
.product-item image{
 display: block;
 width: 100%;
 height: 100%;

}
.product-box::after{
  content: '';
  clear: both;
  display: block;
}
.code image{
  width:100%;
  height: 188rpx;
  margin: 50rpx 0 20rpx;
}
.swiper{
  position: relative;
  height: 188rpx;
}
